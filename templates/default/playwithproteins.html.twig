{% extends 'base.html.twig' %}
{% block body %}
    <div class="container">
        <h1>Play with sequencies</h1>
        <br>
        <h4>Sample code : </h4>
        <br>
        {{ code | raw }}
        <br>
        <h4>Result :</h4>
        <div class="row">
            <div class="col-md-10">
                <p>Returns the length of a protein :</p>
                {{ dump(length) }}</div>
        </div>
        <div class="row">
            <div class="col-md-10">
                <p>Returns the molweight of a protein :</p>
                {{ dump(molwt) }}</div>
        </div>
    </div>
{% endblock %}